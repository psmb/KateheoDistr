prototype(Sfi.Kateheo:Author) {
	main = TYPO3.Neos:ContentCollection{
		nodePath = 'main'
	}
	newsList = Sfi.Listable:List {
		@context.listClass = 'news-list-view'
		prototype(Sfi.Listable:Listable) {
			collection = ${q(site).find('[instanceof Sfi.Listable:ListableMixin]').filterByReference('author', documentNode).get()}
		}
	}

	@process.contentElementWrapping = TYPO3.Neos:ContentElementWrapping
}
